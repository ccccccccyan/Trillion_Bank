<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ac">
	<insert id="account_insert" parameterType="vo.AccountVO">
		insert into ACCOUNT_LIST values(
					#{account_number},
					#{user_id},
					#{bank_name},
					#{now_money},
					#{account_pwd}
		)
	</insert>
	
	<select id="account_list" resultType="vo.AccountVO" parameterType="String">
		select * from ACCOUNT_LIST where user_id = #{user_id}
	</select>
	
	<select id="account_selectOne" resultType="vo.AccountVO" parameterType="String">
		select * from ACCOUNT_LIST where account_number = #{account_number}
	</select>
	
</mapper>












