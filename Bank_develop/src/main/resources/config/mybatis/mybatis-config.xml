<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>
	
	<typeAliases>
		<typeAlias type="vo.AccountdetailVO" alias="account_detail"/>  
		<typeAlias type="vo.AccountVO" alias="account"/>  
		<typeAlias type="vo.CommentVO" alias="comm"/>  
		<typeAlias type="vo.NoticeVO" alias="notice"/>  
		<typeAlias type="vo.QnaVO" alias="qna"/>   
		<typeAlias type="vo.RateboardVO" alias="rateboard"/>  
		<typeAlias type="vo.RateVO" alias="rate"/>  
		<typeAlias type="vo.UserVO" alias="user"/>  
		<typeAlias type="vo.ProductVO" alias="product"/>
		<typeAlias type="vo.Foreign_exchangeVO" alias="exchange"/>
	</typeAliases>
	
	<mappers>
	  <mapper resource="config/mybatis/mapper/account.xml" /> 
	  <mapper resource="config/mybatis/mapper/rate.xml" /> 
	  <mapper resource="config/mybatis/mapper/user.xml" /> 
	  <mapper resource="config/mybatis/mapper/notice.xml" />
 	  <mapper resource="config/mybatis/mapper/qna.xml" />
 	  <mapper resource="config/mybatis/mapper/rateBoard.xml" />
		<mapper resource="config/mybatis/mapper/comment.xml" />
	</mappers>
</configuration>